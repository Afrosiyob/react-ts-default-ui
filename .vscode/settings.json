{
  "cSpell.words": [
    "browserslist",
    "camelcase",
    "commitlint",
    "languagedetector",
    "stylelint",
    "TTFB"
  ],
  "editor.minimap.enabled": true,
  "explorer.compactFolders": false,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "**/package-lock.json": true
  }
}
